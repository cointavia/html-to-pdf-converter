{
    "name": "cointavia/html-to-pdf-converter",
    "description": "Convert HTML code into PDF File",
    "keywords": [
        "laravel",
        "HTML",
        "api",
        "PDF",
        "PDF Converter",
        "HTML Conversion",
        "documentation",
        "BlockChain",
        "Ethereum"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cointavia - Gayan Jayanath",
            "email": "admin@cointavia.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cointavia\\PDF\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cointavia\\PDF\\CointaviaPDFServiceProvider"
            ],
            "aliases": {
                "CointaviaPDF": "Cointavia\\PDF\\Facades\\CointaviaPdfFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
